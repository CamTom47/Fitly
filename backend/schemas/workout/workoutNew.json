{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "name",
        "user_id",
        "category"
    ],
    "properties": {
        "name": {
            "type": "string",
            "default": "",
            "title": "The name Schema",
            "examples": [
                "Leg Day 1"
            ]
        },
        "user_id": {
            "type": "integer",
            "default": 0,
            "title": "The user_id Schema",
            "examples": [
                1
            ]
        },
        "category": {
            "type": "string",
            "default": "",
            "title": "The category Schema",
            "examples": [
                "Strength"
            ]
        }
    },
    "examples": [{
        "name": "Leg Day 1",
        "user_id": 1,
        "category": "Strength"
    }]
}